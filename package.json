    "sequelize": "4.43.0",
    "socket.io": "2.2.0",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-plugin-root-import": "6.1.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-stage-0": "6.24.1",
    "codecov": "3.1.0",
    "cross-env": "5.2.0",
    "eslint": "5.12.1",
