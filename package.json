{
  "name": "backend-starter-kit",
  "version": "1.0.0",
  "description": "A boilerplate for Node.js, Express, Mongoose, Heroku, mLab, Nodemon, PM2, and Babel.",
  "engines": {
    "node": "10"
  },
  "scripts": {
    "start": "nodemon src/api.js --exec babel-node",
    "build": "babel src -d dist",
    "lint": "eslint 'src/**/*.js'",
    "test": "jest \"src/.*\\.js$\" --coverage --forceExit",
    "e2e": "cross-env JEST_ENV=e2e jest \"src/.*\\.js$\" --coverage --forceExit",
    "codecov": "codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shyam-Chen/Backend-Starter-Kit.git"
  },
  "keywords": [
    "Node.js"
  ],
  "author": "Yen-cheng Chen, 陳彥澄",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shyam-Chen/Backend-Starter-Kit/issues"
  },
  "homepage": "https://github.com/Shyam-Chen/Backend-Starter-Kit#readme",
  "dependencies": {
    "@tensorflow/tfjs": "0.15.3",
    "@tensorflow/tfjs-node": "0.3.1",
    "apollo-server-express": "2.4.8",
    "bcrypt": "3.0.4",
    "chalk": "2.4.2",
    "cloudinary": "1.13.2",
    "compression": "1.7.3",
    "connect-redis": "3.4.0",
    "cors": "2.8.5",
    "express": "4.16.4",
    "express-history-api-fallback": "2.2.1",
    "express-session": "1.15.6",
    "formidable": "1.2.1",
    "graphql": "14.1.1",
    "graphql-tag": "2.10.1",
    "helmet": "3.15.1",
    "ioredis": "^4.6.2",
    "joi": "14.3.1",
    "jsonwebtoken": "8.5.0",
    "lodash": "4.17.11",
    "merge-graphql-schemas": "1.5.8",
    "mongoose": "5.4.17",
    "morgan": "1.9.1",
    "node-schedule": "1.3.2",
    "nodemailer": "5.1.1",
    "passport": "0.4.0",
    "passport-facebook": "3.0.0",
    "passport-google-oauth20": "1.0.0",
    "passport-jwt": "4.0.0",
    "passport-twitter": "1.0.4",
    "pg": "7.8.1",
    "pg-hstore": "2.3.2",
